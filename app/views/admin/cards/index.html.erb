<% @cards.each do |card| %>
  <div class='card-wrapper'>
    <%= render partial: 'cards/card', locals: { card: card } %>

    <%= link_to("Delete", admin_card_path(card), { method: :delete,
                                                 remote: true,
                                                 class: "delete-card" })
    %>
  </div>
<% end %>
